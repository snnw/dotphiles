[DEFAULT]
#include = cat /usr/share/mr/lib/git-svn

lib = 
    hostname="$(hostname)"
    whoami="$(whoami)"
    on() {
        for host in $@; do
            if [ "${host%@*}" != "${host#*@}" ]; then
                if [ "$whoami" != "${host%@*}" ]; then
                    continue
                fi
                host="${host#*@}"
            fi
            if [ "$hostname" = "$host" ]; then
                return 0
            fi
        done
        return 1
    }
    mylaptop() {
        on snnw@cthulu
    }

zap =
	git fetch upstream
	git merge upstream/master
	git push origin master

[life]
checkout = git clone 'git@bitbucket.org:snnw/life.git' 'life'
skip = ! mylaptop

