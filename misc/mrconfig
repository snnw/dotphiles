[src/code/xmas]
checkout = git clone git@bitbucket.org:snnw/xmas.git xmas
	cd xmas
	git init -s --username swouda https://svn.cs.ru.nl/repos/xmas
	git svn fetch
zap = git svn fetch
	git merge origin/trunk
	git push origin develop

